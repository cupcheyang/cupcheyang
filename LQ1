#include <stdio.h>
int main()
{
	int n,m,i,j;
	char a[100][100];
	scanf("%d%d",&n,&m);
	for(i=0;i<n;i++)
	{
	for(j=0;j<m;j++)
	{
		if(j==i)
		{
		a[i][j]='A';
		}
		else if(i>j) 
		{
		a[i][j]='A'+(i-j)%26;
		}
		else
		 {
		a[i][j]='A'+(j-i)%26;
		}
	}
	} 
	for(i=0;i<n;i++)
	{for(j=0;j<m;j++)
	
	{
		printf("%c",a[i][j]);
	}
		printf("\n");
	}
		
	return 0;
}
